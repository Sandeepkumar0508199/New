select BatchId, LoadName, Asset,runid from (
select LoadName,batchId,Asset,runid ,ROW_NUMBER() over (partition by batchid order by LoadName ) as nbr 
from [dbo].[TDRLoadstatusView]
where BatchId = '@{pipeline().parameters.RUNProcessDate}'
and LoadName in ('CM FX Trades', 'NYB FX Trades', 'NYB Swap Trades', 'CM Swap Trades')
and LoadStatus='Succeeded'
) a where nbr=1
