select BatchId, LoadName, Asset,runid from
(
select runid
,LoadName,batchId,Asset ,ROW_NUMBER() over (partition by batchid order by LoadName ) nbr
from [dbo].[TDRLoadstatusView]
where
LoadName in ('CME-WellsFargo') and LoadStatus in ('Succeeded')
and BatchId = CASE 
    WHEN '@{pipeline().parameters.RUNProcessDate}' = '' 
    THEN (select min(batchid) from [dbo].[TDRLoadstatusView] where LoadName in ('CME-WellsFargo') and LoadStatus='Succeeded')
    ELSE CAST(REPLACE('@{pipeline().parameters.RUNProcessDate}', '-', '') AS INT)
END
) a where nbr=1
--order by BatchId
